<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ConstraintLayout1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/card_margin"
        android:layout_marginEnd="@dimen/card_margin"
        app:cardElevation="@dimen/card_margin"
        app:cardUseCompatPadding="true"
        android:foreground="?android:attr/selectableItemBackground"
        android:clickable="true"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/textView3">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/card_padding"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/stock_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:text="TextView"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/calc_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/calc"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:gravity="center_vertical"
                    android:text="@null"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/calc_img"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/divider"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout6" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/card_padding"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider">


                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/holding_amount"
                    tools:layout_editor_absoluteX="89dp"
                    tools:layout_editor_absoluteY="21dp" />

                <TextView
                    android:id="@+id/holding_amount"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    tools:layout_editor_absoluteX="116dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/realtime_price"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/realtime_price"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/syncing"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/holding_cost"
                    tools:layout_editor_absoluteX="58dp"
                    tools:layout_editor_absoluteY="21dp" />

                <TextView
                    android:id="@+id/holding_cost"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/holding_calc"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/holding_calc"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/average_buy"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/average_buy"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/profit_calc"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/profit_calc"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/syncing"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/profit_rate"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/profit_rate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/syncing"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@null"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <TextView
                    android:id="@+id/profit_rate2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@null"
                    tools:layout_editor_absoluteX="174dp"
                    tools:layout_editor_absoluteY="60dp" />

                <androidx.constraintlayout.helper.widget.Flow
                    android:id="@+id/flow"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:constraint_referenced_ids="textView11,textView6,holding_amount,realtime_price,textView18,textView12,holding_calc,holding_cost,textView17,textView7,average_buy,profit_calc,textView21,textView23,profit_rate,profit_rate2"
                    app:flow_horizontalAlign="start"
                    app:flow_horizontalBias="0.5"
                    app:flow_horizontalStyle="spread"
                    app:flow_maxElementsWrap="2"
                    app:flow_wrapMode="chain"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>